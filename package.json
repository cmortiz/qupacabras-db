{
  "name": "qupacabras-db",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://cmortiz.github.io/qupacabras-db",
  "dependencies": {
    "lucide-react": "^0.395.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1",
    "serve": "^14.2.4"
  },
  "scripts": {
    "start": "cross-env NODE_OPTIONS=\"--experimental-webstorage --localstorage-file=/tmp/qupacabras-localstorage.db\" react-scripts start",
    "build": "cross-env NODE_OPTIONS=\"--experimental-webstorage --localstorage-file=/tmp/qupacabras-localstorage.db\" node scripts/generate-benchmark-index.js && cross-env NODE_OPTIONS=\"--experimental-webstorage --localstorage-file=/tmp/qupacabras-localstorage.db\" react-scripts build",
    "test": "cross-env NODE_OPTIONS=\"--experimental-webstorage --localstorage-file=/tmp/qupacabras-localstorage.db\" react-scripts test",
    "test:ci": "cross-env NODE_OPTIONS=\"--experimental-webstorage --localstorage-file=/tmp/qupacabras-localstorage.db\" react-scripts test --coverage --ci --watchAll=false",
    "generate-index": "node scripts/generate-benchmark-index.js",
    "validate": "node scripts/validate-benchmark.js",
    "validate:file": "node scripts/validate-benchmark.js --file",
    "validate:all": "node scripts/validate-benchmark.js",
    "prepare": "husky"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "cross-env": "^10.1.0",
    "husky": "^9.1.7"
  }
}
